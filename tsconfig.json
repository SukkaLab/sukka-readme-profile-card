{
  "compilerOptions": {
    "composite": true,
    "target": "esnext",
    "module": "preserve",
    "moduleResolution": "bundler",
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  },
  "include": [
    "./index.ts",
    "./github-stats-json.ts"
  ],
  "exclude": ["node_modules"]
}
